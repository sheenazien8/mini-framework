{
    "name": "sheenazien8/zien-lite",
    "description": "Php OOP Boilerplate",
    "keywords": ["mvc", "framework", "lite"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "sheenazien",
            "email": "sheenazien08@gmail.com"
        }
    ],
    "require": {
        "zien/zien-lite": "dev-master",
        "php": ">=5.6.0",
        "nikic/fast-route": "^1.3",
        "vlucas/phpdotenv": "^2.5",
        "slim/pdo": "~1.10",
        "jenssegers/blade": "^1.1",
        "filp/whoops": "^2.3",
        "robmorgan/phinx": "^0.10.6"
