{
    "name": "zien/zien-lite",
    "description": "Zien Lite Framework",
    "keywords": ["mvc", "framework", "lite"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "sheenazien",
            "email": "sheenazien08@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "nikic/fast-route": "^1.3",
